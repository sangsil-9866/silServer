    @Id
    private String id;

    @Comment("아이디")
    private String username;

    @Comment("비밀번호")
    private String password;

    @Comment("이름")
    private String name;

    @Comment("이메일")
    private String email;

    @Comment("권한")
    private UserRole role;

    @Comment("가입일시")
    private LocalDateTime signupAt = LocalDateTime.now();

    @Comment("로그인일시")
    private LocalDateTime signindAt;
